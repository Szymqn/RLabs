set_rownames(y_axis)
for (i in seq_along(y_axis)) {
for (j in seq_along(x_axis)) {
t_table[i, j] <- signif(qt(1 - x_axis[j]/2, y_axis[i]),digits = 5)
}
}
t_table
datatable(t_table, caption = 'Tabela: Tablica rozkładu t-Studenta.')
