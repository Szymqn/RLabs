library(tidyverse)
library(DT)
install.packages("DT")
datatable(t_tabela, caption = 'Tabela: Tablica razkładu t-Studenta.')
# Determine range and interval width
data_range <- max(data) - min(data)
library(tidyverse)
library(DT)
data <- c(12, 15, 17, 20, 23, 25, 28, 29, 32, 35)
# Determine range and interval width
data_range <- max(data) - min(data)
interval_width <- 5
# Create intervals
intervals <- seq(from=min(data), to=max(data), by=interval_width)
# Initialize frequency and cumulative frequency vectors
frequency <- rep(0, length(intervals)-1)
cumulative_frequency <- rep(0, length(intervals)-1)
# Count data points in each interval
for (d in data) {
for (i in 1:length(intervals)-1) {
if (intervals[i] <= d & d < intervals[i+1]) {
frequency[i] <- frequency[i] + 1
break
}
}
}
# Calculate relative and cumulative frequencies
total_data_points <- length(data)
relative_frequency <- frequency / total_data_points
cumulative_frequency <- cumsum(frequency)
# Create data frame with distribution table
df <- data.frame(
Interval = paste(intervals[-length(intervals)], intervals[-1], sep="-"),
Frequency = frequency,
Relative_Frequency = round(relative_frequency, 2),
Cumulative_Frequency = cumulative_frequency
)
# Print distribution table
print(df)
rt(n, df)
n <- 30
df <- n - 1
rt(n, df)
rt(n, df)
